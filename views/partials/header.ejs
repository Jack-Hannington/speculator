<body>
  <style>
.summary-card {
    position: relative;
}

.ct-chart {
    width: 120px;
    height: 50px; /* Adjust the height to your preference */
    position: absolute;
    bottom: 10px; /* Adjust the positioning as needed */
    right: 10px; /* Adjust the positioning as needed */
}

.ct-series-a .ct-line {
    stroke: rgba(75, 192, 192, 1);
    stroke-width: 2px;
    stroke-linecap: round;
}

.ct-series-a .ct-area {
    fill: rgba(75, 192, 192, 0.2);
}

</style>

<script>
  new Chartist.Line('.ct-chart', {
      labels: Array(6).fill(''), // Hidden labels
      series: [
          [2, 4, 6, 7, 6, 8]
      ]
  }, {
      showPoint: false,
      lineSmooth: Chartist.Interpolation.simple(),
      axisX: {
          showLabel: false,
          showGrid: false
      },
      axisY: {
          showLabel: false,
          showGrid: false
      },
      fullWidth: true,
      chartPadding: {
          right: 0,
          left: 0,
          top: 0,
          bottom: 0
      },
      showArea: true
  });
</script>